{
  "name": "Oncf",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Train Extension for morrocan cities",
  "browser_action": {
    "default_icon": "img/ico.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/jquery-3.1.1.min.js", "js/bootstrap.min.js", "js/moment.js", "js/background.js", "js/bootstrap-toggle.min.js"]
  },	
  "icons": {
    "128" : "img/ico.png"
  },
  "permissions": [	
	"storage",
	"tabs", 
	"activeTab",  
	"http://*/*",
	"https://*/*",
    "notifications",
    "alarms"
  ]
}